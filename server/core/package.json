{
  "name": "rdvc-server-core",
  "version": "0.0.0",
  "private": true,
  "bin": "./build/server/core/src/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "prod": "node ./build/server/core/src/index.js",
    "pkg": "pkg . --output=bin/rdvc-server-core"
  },
  "pkg": {
    "assets": [
      "views/**/*"
    ],
    "scripts": "./build/server/core/src/index.js",
    "targets": [
      "node16-macos-x64",
      "node16-win-x64",
      "node12-linux"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "fs": "*",
    "https": "^1.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/ws": "^8.5.5",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
